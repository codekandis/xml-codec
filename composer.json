{
	"name": "codekandis/xml-codec",
	"description": "`codekandis/xml-codec` is a library providing a codec to encode into and decode from XML in an object oriented way.",
	"keywords": [
		"php",
		"xml codec",
		"xml",
		"codec",
		"encode",
		"decode"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/xml-codec/issues",
		"docs": "https://github.com/codekandis/xml-codec/blob/master/README.md",
		"source": "https://github.com/codekandis/xml-codec/tree/master"
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\XmlCodec\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\XmlCodec\\Tests\\": [
				"tests/"
			]
		}
	}
}
